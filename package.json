{
  "name": "orthoiq-agents",
  "version": "1.0.0",
  "description": "AI-powered agent framework for orthopedic medical applications",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "example": "node examples/basic-agent.js",
    "compile:contract": "hardhat compile",
    "deploy:contract": "hardhat run scripts/deploy.js --network base-sepolia",
    "authorize:agents": "hardhat run scripts/authorize-agents.js --network base-sepolia",
    "check:balances": "hardhat run scripts/check-balances.js --network base-sepolia",
    "verify:contract": "hardhat verify --network base-sepolia"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.29.0",
    "@coinbase/agentkit": "^0.10.2",
    "@coinbase/agentkit-langchain": "^0.3.0",
    "@coinbase/cdp-agentkit-core": "^0.0.14",
    "@coinbase/cdp-langchain": "^0.0.15",
    "@coinbase/cdp-sdk": "^1.38.1",
    "@langchain/anthropic": "^0.3.7",
    "@langchain/core": "^0.3.21",
    "@langchain/langgraph": "^0.2.16",
    "bull": "^4.16.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.4",
    "express": "^4.21.1",
    "express-sse": "^1.0.0",
    "lru-cache": "^11.2.2",
    "node-fetch": "^3.3.2",
    "p-queue": "^8.1.1",
    "p-timeout": "^7.0.0",
    "uuid": "^11.0.3",
    "winston": "^3.15.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.15.0",
    "hardhat": "^2.22.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "ai",
    "agents",
    "orthopedic",
    "medical",
    "blockchain",
    "langchain"
  ],
  "author": "OrthoIQ Team",
  "license": "MIT"
}
